{
  "name": "electron-demo",
  "productName": "Electron tutorial app",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "start:dev": "react-scripts start",
    "build:react": "webpack --mode development --config webpack.react.config.js",
    "build": "webpack --mode development",
    "start": "electron ./dist/main.js",
    "start:electron": "webpack --mode development --config webpack.react.config.js && webpack --mode development && electron ./dist/main.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/powershell.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "prepare": "npm run snyk-protect",
    "snyk-protect": "snyk-protect"
  },
  "keywords": [],
  "author": "Dmitry Druzhkov",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.5",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "css-loader": "^2.1.1",
    "electron": "^4.2.0",
    "electron-packager": "^13.1.1",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.17.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "@types/reactstrap": "^8.0.1",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "node-powershell": "^4.0.0",
    "node-sass-chokidar": "^1.3.4",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-scripts": "^3.0.1",
    "reactstrap": "^8.0.0",
    "rxjs": "^6.5.2",
    "@snyk/protect": "latest"
  },
  "snyk": true
}
